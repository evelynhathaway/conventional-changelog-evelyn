{
  "name": "conventional-changelog-evelyn",
  "version": "1.3.1",
  "description": "Conventional Changelog and Semantic Release presets for my projects",
  "keywords": [
    "conventional-changelog",
    "semantic-release-config",
    "preset"
  ],
  "bugs": {
    "url": "https://github.com/evelynhathaway/conventional-changelog-evelyn/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evelynhathaway/conventional-changelog-evelyn.git"
  },
  "license": "MIT",
  "author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
  "main": "./index.js",
  "files": [
    "templates/*",
    "*.js",
    "!.eslintrc.js"
  ],
  "scripts": {
    "lint": "eslint ./",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "./": "eslint --fix"
  },
  "dependencies": {
    "compare-func": "^2.0.0",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "commitlint": "^12.1.4",
    "eslint": "^7.27.0",
    "eslint-plugin-evelyn": "^6.2.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-regexp": "^0.11.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "semantic-release": "^17.4.3",
    "sort-package-json": "^1.50.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
